{
  "name": "user-management",
  "version": "1.0.0",
  "description": "This is a user management system",
  "main": "server.js",
  "scripts": {
    "prestart": "npm install",
    "predev": "npm install",
    "start": "node server.js",
    "dev": "node server.js",
    "lint:fix": "eslint --fix .",
    "test": "jest --config=jest.config.js --forceExit --watchAll",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "migrate": "node-pg-migrate",
    "up": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo",
    "down:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-plugin-snakecasejs": "^2.2.0"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  }
}
